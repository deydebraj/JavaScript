//geneate a random decimal number in javaScript
function randomFunction(){
    return Math.random();
}
console.log(randomFunction());

//geneate a random whole number in javaScript
function randomFunction(){
    return Math.floor(Math.random()*10);        //between 1 to 10, but not 0
}
console.log(randomFunction());


//geneate a random whole number between two values in javaScript

function randomFunctionBetween(min,max){
    return Math.floor(Math.random()*(max - min + 1)+ min);
}
console.log(randomFunctionBetween(5,20));
